<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>10</LangVersion>
        <EnforceExtendedAnalyzerRules>true</EnforceExtendedAnalyzerRules>
        <BuildOutputTargetFolder>analyzers</BuildOutputTargetFolder>
        <IncludeBuildOutput>false</IncludeBuildOutput>
        <IsRoslynComponent>true</IsRoslynComponent>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Workspaces" Version="4.4.0" />
        <PackageReference Include="ThisAssembly" Version="1.2.12" PrivateAssets="all" />
    </ItemGroup>

    <ItemGroup>
      <Compile Include="..\Futurum.Microsoft.Extensions.DependencyInjection\DuplicateRegistrationStrategy.cs">
        <Link>DuplicateRegistrationStrategy.cs</Link>
      </Compile>
    </ItemGroup>

</Project>
